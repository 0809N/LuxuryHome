    <section class="contact-section">
        <div class="container">
            <div class="row">
                <div class="col-lg-7 offset-lg-5">
                    <div class="contact-info-box">
                        <h5 class="contact-title">Thông tin</h5>
                        <div class="contact-info">
                            <br>
                            <div class="ci-item">
                                <img src="/Content/img/icons/location.png" class="ci-icon" alt="">
